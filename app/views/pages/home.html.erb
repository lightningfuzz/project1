
<h1>Michael Santer</h1>

<table class="table table-hover">
	<thead>
		<tr>
			<th>Project Name</th><th>Description</th><th>URL</th>
		</tr>
	</thead>
	<tbody>
			<%PROJECTS.each do |project, details|%>
			<tr>
			<td><%=project%></td>
			<td><%=details['description']%></td>
			<% url = details['url'] %>
			<td><a href=<%=url%>><%=url%></a></td>
			</tr>
		<%end%>

		</tr>
	</tbody>
</table>

